g1=[]
g2=[]
g3=[]
g4=[]
g5=[]
size=int(input('What is the size of group 1:'))
g1.append(size)
date=input('When did group 1 do the competition:')
g1.append(date)
venue=input('Where did group 1 do the competition')
g1.append(venue)
medal=input('What medal did group 1 get:')
g1.append(medal)
g1=tuple(g1)
print(g1)
